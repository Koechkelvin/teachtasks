#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Dev-Cpp/laikipia/git assignments/teachtasks/RestfulAPI/node_modules/touch/bin/node_modules:/mnt/c/Dev-Cpp/laikipia/git assignments/teachtasks/RestfulAPI/node_modules/touch/node_modules:/mnt/c/Dev-Cpp/laikipia/git assignments/teachtasks/RestfulAPI/node_modules:/mnt/c/Dev-Cpp/laikipia/git assignments/teachtasks/node_modules:/mnt/c/Dev-Cpp/laikipia/git assignments/node_modules:/mnt/c/Dev-Cpp/laikipia/node_modules:/mnt/c/Dev-Cpp/node_modules:/mnt/c/node_modules:/mnt/c/Dev-Cpp/laikipia/git assignments/teachtasks/RestfulAPI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Dev-Cpp/laikipia/git assignments/teachtasks/RestfulAPI/node_modules/touch/bin/node_modules:/mnt/c/Dev-Cpp/laikipia/git assignments/teachtasks/RestfulAPI/node_modules/touch/node_modules:/mnt/c/Dev-Cpp/laikipia/git assignments/teachtasks/RestfulAPI/node_modules:/mnt/c/Dev-Cpp/laikipia/git assignments/teachtasks/node_modules:/mnt/c/Dev-Cpp/laikipia/git assignments/node_modules:/mnt/c/Dev-Cpp/laikipia/node_modules:/mnt/c/Dev-Cpp/node_modules:/mnt/c/node_modules:/mnt/c/Dev-Cpp/laikipia/git assignments/teachtasks/RestfulAPI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
